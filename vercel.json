{
  "version": 2,
  "buildCommand": "npm ci && npm run build",
  "installCommand": "npm ci",
  "outputDirectory": "dist/insurance-quote-tool",
  "framework": "angular",
  "env": {
    "NODE_VERSION": "20.x"
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
